@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix  rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix crm: <http://www.cidoc-crm.org/cidoc-crm/> .
@prefix schema: <http://schema.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.com/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

# Name, StartDate, location_name, location_address, ticket_url

<#WikipediaExportMapping>
  rml:logicalSource [
    rml:source "/events-listing.csv";
    rml:referenceFormulation ql:CSV;
  ];
  rr:subjectMap [
    rr:template "http://ex.com/{Name}";
    rr:class schema:MusicEvent ;
  ];

  rr:predicateObjectMap 
  [
    rr:predicate schema:name;
    rr:objectMap 
    [
      rml:reference "Name";
      rr:datatype xsd:date;
    ]
  ];


  rr:predicateObjectMap 
  [
    rr:predicate schema:x-locAddr;
    rr:objectMap 
    [
      rml:reference "location_address";
      rr:datatype xsd:date;
    ]
  ];

  rr:predicateObjectMap 
  [
    rr:predicate schema:x-locName;
    rr:objectMap 
    [
      rml:reference "location_name";
      rr:datatype xsd:date; 
    ]
  ].
